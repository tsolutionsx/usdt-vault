contract Vault is Ownable, ReentrancyGuard {
    using Address for address;
    using Strings for uint256;

    IERC20 public token;

    IERC721 public t1;
    IERC721 public t2;
    IERC721 public t3;

    struct Stake {
        address account;
        uint256 amount;
        uint256 boostedAmount;
        uint256 timestamp;
    }

    mapping(address => Stake) public userToStake;
    mapping(address => bool) public userToIsStaked;
    mapping(address => uint256) public nftToBoostRate;

    uint256 public minClaimStakePeriodUnit = 1 days;
    uint256 public totalStakedAmount;

    uint256 public constant T1_BOOST_RATE = 40;
    uint256 public constant T2_BOOST_RATE = 20;
    uint256 public constant T3_BOOST_RATE = 5;
    uint256 public constant VAULT_PERIOD = 182 days;

    uint256 public valutStartedTimestamp;

    event Deposit(address indexed account, uint256 amount, uint256 boostedAmount, uint256 timestamp);
    event Claim(address indexed account, uint256 amount, uint256 timestamp);

    modifier checkVaultTerminated() {
        require(block.timestamp - valutStartedTimestamp < VAULT_PERIOD, "VT#4");
        _;
    }

    constructor(address _token, address _t1, address _t2, address _t3) {
        token = IERC20(_token);

        t1 = IERC721(_t1);
        t2 = IERC721(_t2);
        t3 = IERC721(_t3);

        nftToBoostRate[_t1] = T1_BOOST_RATE;
        nftToBoostRate[_t2] = T2_BOOST_RATE;
        nftToBoostRate[_t3] = T3_BOOST_RATE;

        valutStartedTimestamp = block.timestamp;
    }

    function deposit(uint256 _amount) external checkVaultTerminated {
        token.transferFrom(_msgSender(), address(this), _amount);

        uint256 boostRate = getBoostRate(_msgSender());
        uint256 boostedDepositAmount = _amount + _amount / 100 * boostRate;

        Stake memory stake = Stake(_msgSender(), _amount, boostedDepositAmount, block.timestamp);
        
        userToStake[_msgSender()] = stake;
        userToIsStaked[_msgSender()] = true;

        totalStakedAmount += boostedDepositAmount;

        emit Deposit(_msgSender(), _amount, boostedDepositAmount, block.timestamp);
    }

    function claim() external nonReentrant {
        require(userToIsStaked[_msgSender()], "VT#0");
        
        Stake memory stake = userToStake[_msgSender()];
        uint256 timespent = block.timestamp - stake.timestamp;
        require(timespent >= minClaimStakePeriodUnit, "VT#1");

        uint256 share = stake.boostedAmount / totalStakedAmount * 100;
        uint256 spentDays = getDaysFromTimestamp(timespent);
        uint256 totalShare = share * spentDays;
        uint256 claimAmount = stake.boostedAmount / 100 * totalShare;

        require(token.balanceOf(address(this)) >= claimAmount, "VT#3");

        token.transfer(_msgSender(), claimAmount);

        userToIsStaked[_msgSender()] = false;
        delete userToStake[_msgSender()];

        emit Claim(_msgSender(), claimAmount, block.timestamp);
    }
    
    function simulateClaim(address _address) external view returns(
        uint256 share_, 
        uint256 spentdays_, 
        uint256 totalShare_, 
        uint256 claimAmount_) {
        require(userToIsStaked[_address], "VT#0");
        
        Stake memory stake = userToStake[_address];
        uint256 timespent = block.timestamp - stake.timestamp;
        require(timespent >= minClaimStakePeriodUnit, "VT#1");

        share_ = stake.boostedAmount / totalStakedAmount * 100;
        spentdays_ = getDaysFromTimestamp(timespent);
        totalShare_ = share_ * spentdays_;
        claimAmount_ = stake.boostedAmount / 100 * totalShare_;
    }

    function getBoostRate(address _address) internal view returns(uint256 boostRate_){
        uint256 t1BoostRate = T1_BOOST_RATE * t1.balanceOf(_address);
        uint256 t2BoostRate = T1_BOOST_RATE * t2.balanceOf(_address);
        uint256 t3BoostRate = T3_BOOST_RATE * t3.balanceOf(_address);

        boostRate_ = t1BoostRate + t2BoostRate + t3BoostRate;
    }

    function getDaysFromTimestamp(uint256 _timestamp) internal pure returns(uint256 days_) {
        uint256 dayInSecond = 60 * 60 * 24;
        require(_timestamp >= dayInSecond, "VT#2");

        days_ = (_timestamp - (_timestamp % dayInSecond)) / dayInSecond;
    }
}